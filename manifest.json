{
  "name": "moonseok",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "moonseok",
  "homepage_url": "https://moonseoklee.kro.kr",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Midas",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ]
}