{
  "name": "필더폼",
  "description" : "Base Level Extension",
  "version": "1.0",
  "browser_action": {
    "default_popup": "home.html"
  },
  "permissions": [
    "activeTab","tabs","browserAction","storage"
  ],
  "background": {
    "scripts": ["index.js"],"persistent": false
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}